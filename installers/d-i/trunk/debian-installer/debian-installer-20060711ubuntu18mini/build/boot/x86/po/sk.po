#: help.xml:173
msgid "Available boot methods:"
msgstr "Dostupné metódy zavádzania:"

#: help.xml:177
msgid "install"
msgstr "install"

#: help.xml:179
msgid "Start the installation -- this is the default ${MEDIA_TYPE} install."
msgstr "Spustiť inštaláciu -- toto je predvolená inštalácia ${MEDIA_TYPE}"

#: help.xml:184
msgid "expert"
msgstr "expert"

